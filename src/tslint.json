{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "sammys",
            "kebab-case"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "no-inferrable-types": false
    }
}